{
  "info": {
    "name": "GLUU BLIP2 Async API",
    "description": "Test collection for GLUU FastAPI Async SageMaker Inference",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/status",
          "host": ["{{base_url}}"],
          "path": ["status"]
        }
      },
      "response": []
    },
    {
      "name": "Analyze Images (Async)",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "files",
              "type": "file",
              "src": "/path/to/test-image.jpg"
            }
          ]
        },
        "url": {
          "raw": "{{base_url}}/analyze-images",
          "host": ["{{base_url}}"],
          "path": ["analyze-images"]
        }
      },
      "response": []
    }
  ]
}
